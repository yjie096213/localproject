package com.htinf.sm.common.model;

/**
 * @ClassName: Page
 * @ProjectName: server_manage
 * @Description:
 * @Author: Administrator
 * @DATE: 2021/7/5 16:02
 **/
public class Page {

    private Integer page_size; // 每页数量
    private Integer page_index; // 当前页数
    private Integer total_page; // 总页数
    private Long    total_size; // 总记录数

    public Page() {

    }

    public Page(Integer page_index, Integer page_size) {
        this.page_index = page_index;
        this.page_size = page_size;
    }

    public Page(Integer page_index, Integer page_size, Integer total_page, Long total_size) {
        this.page_index = page_index;
        this.page_size = page_size;
        this.total_page = total_page;
        this.total_size = total_size;
    }

    public Integer getPage_size() {
        return page_size;
    }

    public void setPage_size(Integer page_size) {
        this.page_size = page_size;
    }

    public Integer getPage_index() {
        return page_index;
    }

    public void setPage_index(Integer page_index) {
        this.page_index = page_index;
    }

    public Integer getTotal_page() {
        return total_page;
    }

    public void setTotal_page(Integer total_page) {
        this.total_page = total_page;
    }

    public Long getTotal_size() {
        return total_size;
    }

    public void setTotal_size(Long total_size) {
        this.total_size = total_size;
    }

}

